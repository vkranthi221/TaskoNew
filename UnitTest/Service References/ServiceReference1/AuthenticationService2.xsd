      <xs:element minOccurs="0" name="Name" nillable="true" type="xs:string" />
      <xs:element minOccurs="0" name="NoOfEmployees" type="xs:int" />
      <xs:element minOccurs="0" name="TimeSpentOnApp" type="xs:dateTime" />
